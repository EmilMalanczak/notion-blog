{
    "name": "cch-blog",
    "version": "2.0.0",
    "private": true,
    "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
    "author": "Travis Fischer <travis@transitivebullsh.it>",
    "repository": "transitive-bullshit/nextjs-notion-starter-kit",
    "license": "MIT",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "deploy": "vercel deploy",
        "deps": "run-s deps:*",
        "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
        "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "test": "run-p test:*",
        "test:lint": "yarn eslint --fix --ext .ts --ext .tsx --ext .js .",
        "test:prettier": "prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/server": "^11.11.0",
        "@fisch0920/medium-zoom": "^1.0.7",
        "@fisch0920/use-dark-mode": "^2.4.0",
        "@keyvhq/core": "^1.6.9",
        "@keyvhq/redis": "^1.6.10",
        "@mantine/core": "^6.0.11",
        "@mantine/hooks": "^6.0.11",
        "@mantine/modals": "^6.0.11",
        "@mantine/next": "^6.0.11",
        "@mantine/nprogress": "^6.0.11",
        "@mantine/spotlight": "^6.0.11",
        "@matejmazur/react-katex": "^3.1.3",
        "@react-icons/all-files": "^4.1.0",
        "@types/lodash.throttle": "^4.1.7",
        "@vercel/og": "^0.0.19",
        "classnames": "^2.3.1",
        "clipboard-copy": "^4.0.1",
        "date-fns": "^2.30.0",
        "expiry-map": "^2.0.0",
        "fathom-client": "^3.4.1",
        "format-number": "^3.0.0",
        "got": "^12.0.3",
        "isomorphic-unfetch": "^3.1.0",
        "katex": "^0.16.7",
        "lodash.throttle": "^4.1.1",
        "lqip-modern": "^1.2.0",
        "next": "^12.3.1",
        "notion-client": "^6.15.6",
        "p-map": "^5.3.0",
        "p-memoize": "^6.0.1",
        "posthog-js": "^1.20.2",
        "prismjs": "^1.29.0",
        "react": "^18.2.0",
        "react-body-classname": "^1.3.1",
        "react-dom": "^18.2.0",
        "react-fast-compare": "^3.2.1",
        "react-hotkeys-hook": "^4.4.0",
        "react-image": "^4.1.0",
        "react-lazy-images": "^1.1.0",
        "react-modal": "^3.16.1",
        "react-pdf": "^6.2.2",
        "react-tweet-embed": "^2.0.0",
        "react-use": "^17.4.0",
        "rss": "^1.2.2",
        "zustand": "^4.3.8"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@next/bundle-analyzer": "^12.3.1",
        "@types/lqip-modern": "^1.1.3",
        "@types/node": "^18.8.5",
        "@types/prismjs": "^1.26.0",
        "@types/react": "^18.0.21",
        "@types/react-body-classname": "^1.1.7",
        "@types/react-modal": "^3.16.0",
        "@types/react-pdf": "^6.2.0",
        "@types/rss": "^0.0.30",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "cross-env": "^7.0.2",
        "eslint": "^8.40.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^13.4.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^7.0.0",
        "lint-staged": "^13.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "overrides": {
        "cacheable-request": {
            "keyv": "npm:@keyvhq/core@~1.6.6"
        }
    }
}
